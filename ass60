package day9;

interface Print{
	void display();
}
interface copy{
	void show();
	
}
class Xerox implements Print, copy{
	public void display()
	{
		System.out.println("Displaying");
	}
	public void show() {
		System.out.println("Showing");
	}
	
}

public class multiinheritance {

	public static void main(String[] args) {
	       Xerox obj= new Xerox();
	       obj.show();
	       obj.display();
	       

	}
