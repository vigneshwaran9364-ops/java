package day4;
import java.util.Scanner;
  public class primenumber {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int num1 =sc.nextInt();
        int count=0;
        if(num1 ==0 || num1 ==1)
        {
        	
         System.out.println("Not a prime"); 	
       
        }
        else {
		for(int i=2;i<=num1;i++)
		{
			
			if(num1%i ==0)
			{ 
				count++;
			}
	
		}
		if(count==1)
		{System.out.println("Prime");
	}}}}
