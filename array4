import java.util.*;
public class array04 {
	public static void main(String [] args) { 
	Scanner scan = new Scanner(System.in);
	int size = scan.nextInt();
	int arr[]=new int[size];
	for(int i = 0;i<arr.length;i++)
	{
		arr[i]=scan.nextInt();
	}
	for(int i=0;i<arr.length;i++)
	{
		if(i%2==0)
		{
			System.out.println(arr[i]);
		}
	}
	}}
