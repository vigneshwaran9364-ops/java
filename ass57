package day9;
abstract class Bike{
	
	  abstract void run();
	}
	class Honda extends Bike{
		void run()
		{
			System.out.println("Vehicle");
		}
		void play()
		{
			System.out.println("playing");
		}
	}
	
	public class abstraction{
		public static void main (String[] args) {
			Bike obj=new Honda();
			obj.run();
			Honda obj1=new Honda();
			obj1.play();
		}
}
